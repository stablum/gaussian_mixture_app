{
  "name": "ml-explorer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --config jest.unit.config.js && jest --config jest.component.config.js",
    "test:unit": "jest --config jest.unit.config.js",
    "test:component": "jest --config jest.component.config.js",
    "test:components": "jest --config jest.component.config.js",
    "test:ci": "jest --config jest.unit.config.js --ci --coverage --watchAll=false",
    "test:watch": "jest --config jest.unit.config.js --watch",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "npm run test && npm run test:e2e",
    "export": "next build && next export"
  },
  "dependencies": {
    "@types/d3": "^7.4.3",
    "@types/katex": "^0.16.7",
    "d3": "^7.9.0",
    "katex": "^0.16.22",
    "next": "^14.0.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-katex": "^3.1.0",
    "tailwindcss": "^3.4.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.9",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.0.4",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "playwright": "^1.54.2",
    "postcss": "^8.5.6",
    "ts-jest": "^29.4.0",
    "typescript": "^5.9.2"
  }
}
